        "privacyUrl": "https://www.codefactor.io/privacy",
        "termsOfUseUrl": "https://www.codefactor.io/terms"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "CodeFactor",
        "full": "CodeFactor.io"
