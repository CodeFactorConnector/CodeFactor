            "configurationUrl": "https://www.codefactor.io/MSTeamsConnector",
            "scopes": [
                "team"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "www.codefactor.io"
    ]
}